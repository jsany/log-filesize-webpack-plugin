{
  "name": "@jsany/log-filesize-webpack-plugin",
  "version": "1.1.0",
  "description": "log assets size after webpack build(wirte by typescript)",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib/* && ttsc",
    "watch": "ttsc -w",
    "release": "standard-version"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jsany/log-filesize-webpack-plugin.git"
  },
  "issues": {
    "url": "https://github.com/jsany/log-filesize-webpack-plugin/issues"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "log",
    "assets",
    "filesize",
    "gzip",
    "webpack",
    "plugin"
  ],
  "author": "jiangzhiguo2010",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jsany/log-filesize-webpack-plugin/issues"
  },
  "homepage": "https://github.com/jsany/log-filesize-webpack-plugin#readme",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "**/*.{js,ts,md,json}": [
      "prettier --write"
    ],
    "package.json": [
      "sort-package-json"
    ]
  },
  "dependencies": {
    "chalk": "^4.1.0",
    "cliui": "^6.0.0",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.1.1",
    "@commitlint/config-conventional": "^9.1.1",
    "@types/node": "^14.0.23",
    "@types/rimraf": "^3.0.0",
    "@types/webpack": "^4.41.21",
    "@typescript-eslint/eslint-plugin": "^3.6.1",
    "@typescript-eslint/parser": "^3.6.1",
    "cross-env": "^7.0.2",
    "eslint": "^7.4.0",
    "eslint-config-alloy": "^3.7.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.2",
    "sort-package-json": "^1.44.0",
    "standard-version": "^8.0.2",
    "ttypescript": "^1.5.10",
    "typescript": "^3.9.6",
    "typescript-transform-paths": "^1.1.14"
  },
  "engines": {
    "node": ">=8.10.0"
  }
}
